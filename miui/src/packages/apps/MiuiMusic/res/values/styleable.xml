<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EqualizerView">
        <attr name="curve_color" format="color"/>
        <attr name="curve_shadow_color" format="color"/>
        <attr name="curve_shadow_radius" format="float"/>
    </declare-styleable>

    <declare-styleable name="PreferenceCommon">
        <attr name="summaryOn" format="string" />
        <attr name="summaryOff" format="string" />
    </declare-styleable>

    <declare-styleable name="ComboPreference">
        <attr name="progressKey" format="string" />
        <attr name="title" format="string" />
        <attr name="minName" format="string" />
        <attr name="maxName" format="string" />
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="progressUnit" format="integer" />
        <attr name="convertType">
            <enum name="raw" value="0" />
            <enum name="size" value="1" />
            <enum name="duration" value="2" />
            <enum name="constant" value="3" />
        </attr>
        <attr name="constantValue" format="string"/>
        <attr name="currentPrefix" format="string"/>
    </declare-styleable>

    <declare-styleable name="CascadingView_Layout">
        <attr name="layout_gravity" />
    </declare-styleable>

    <!-- Reference to framework/base/core/res/res/values/attrs.xml #layout_gravity -->
    <attr name="layout_gravity">
        <flag name="top" value="0x30" />
        <flag name="bottom" value="0x50" />
        <flag name="left" value="0x03" />
        <flag name="right" value="0x05" />
        <flag name="center_vertical" value="0x10" />
        <flag name="fill_vertical" value="0x70" />
        <flag name="center_horizontal" value="0x01" />
        <flag name="fill_horizontal" value="0x07" />
        <flag name="center" value="0x11" />
        <flag name="fill" value="0x77" />
        <flag name="clip_vertical" value="0x80" />
        <flag name="clip_horizontal" value="0x08" />
        <flag name="start" value="0x00800003" />
        <flag name="end" value="0x00800005" />
    </attr>
</resources>
